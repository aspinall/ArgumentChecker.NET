    <PackageLicenseUrl>https://raw.githubusercontent.com/aspinall/ArgumentChecker.NET/master/LICENSE</PackageLicenseUrl>
    <PackageProjectUrl>https://github.com/aspinall/ArgumentChecker.NET</PackageProjectUrl>
    <RepositoryUrl>https://github.com/aspinall/ArgumentChecker.NET.git</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <Product>ArgumentChecker</Product>
    <Version>1.0.2</Version>
    <PackageReleaseNotes>Removed .NetStandard as a target framework. Only targeting .NetFramework 4.6.1 now.</PackageReleaseNotes>
  </PropertyGroup>

</Project>
